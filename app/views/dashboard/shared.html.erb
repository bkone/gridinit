<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <meta name="description" content="">
      <meta name="author" content="">
      <title><%= request.subdomain %> Gridinit &raquo; <%= controller.controller_name.capitalize %></title>
      <%= stylesheet_link_tag    "application", :media => "all" %>
      <%= stylesheet_link_tag    "bootstrap", :media => "all" %>
      <%= stylesheet_link_tag    "rickshaw", :media => "all" %>
      <%= stylesheet_link_tag    "prettify", :media => "all" %>
      <link href="/css/smoothness/jquery-ui-1.8.17.custom.css" rel="stylesheet" type="text/css">
      <%= javascript_include_tag "application", "bootstrap", "jquery.jeditable", "jquery.tablesorter", "jquery.validate", "jquery.highlight", "jquery.ui", "json2", "prettify", "d3", "d3.layout", "rickshaw", "jquery.endless-scroll" %>
      <%= csrf_meta_tags %>
      <link href="http://fonts.googleapis.com/css?family=Oxygen" rel="stylesheet" type="text/css">  
      <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
      <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
      <![endif]-->
  </head>    
    <body>

    <div class="secondary-masthead">  
    <div class="container">    
      <ul class="breadcrumb">
          <li class="active">Test Results : <%= params[:domain] %></li>
       </ul>
    </div> 
  </div>

  <div class="main-area dashboard">
    <div class="container">
      
      <% if params[:testguid] %>

        <% if @total_hits > 0 %>
          <div class="row"> 
            <%= render 'stats' %>
          </div>    
          
          <div class="row">
            <%= render 'charts' %>
          </div>
        <% end %>
      <% end %>

      <!-- Footer -->
      <div class="row">
        <div class="span12 footer">
          <h6 class="pull-left">Executed: <%= @run.updated_at.localtime.asctime %></h6> Â© 2012 <a href="http://altentee.com/">Altentee</a> Pty Ltd.
        </div>
      </div>

    </div>
  </div>

 </body>
</html>